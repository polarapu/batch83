touch file1
ls
git status
git add file1  (workspace to staging)
git reset head file  (staging to workspace)
git status
git commit -m "label" file1 (staging to local)
git reset --soft cid  (local to staging)
git reset --mixed cid  (local to workspace)
git reset --hard cid  

git status
git log
git show cid


